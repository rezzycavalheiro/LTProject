<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/xml.xsd" xsi:schemaLocation="http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/web-app_4_0.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CadastroONGController</servlet-name>
        <servlet-class>controller.CadastroONGController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarVoluntarioController</servlet-name>
        <servlet-class>controller.ListarVoluntarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroVoluntarioController</servlet-name>
        <servlet-class>controller.CadastroVoluntarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarONGController</servlet-name>
        <servlet-class>controller.ListarONGController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginONGController</servlet-name>
        <servlet-class>controller.LoginONGController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginVoluntarioController</servlet-name>
        <servlet-class>controller.LoginVoluntarioController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroONGController</servlet-name>
        <url-pattern>/CadastroONGController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarVoluntarioController</servlet-name>
        <url-pattern>/ListarVoluntarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroVoluntarioController</servlet-name>
        <url-pattern>/CadastroVoluntarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarONGController</servlet-name>
        <url-pattern>/ListarONGController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginVoluntarioController</servlet-name>
        <url-pattern>/LoginVoluntarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginONGController</servlet-name>
        <url-pattern>/LoginONGController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
